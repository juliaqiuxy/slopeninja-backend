/* When the user clicks on the button,
toggle between hiding and showing the dropdown content */
function weatherFunction() {
	event.stopPropagation();
	document.getElementById("weatherDropdown").classList.toggle("show");
	document.getElementById("weatherBtn").classList.toggle("open");
}

function removeOpen() {

	var dropdowns = document.getElementsByClassName("dropdown-content");
	var dropdownBtn = document.getElementsByClassName("dropbtn");

	var i;
	for (i = 0; i < dropdowns.length; i++) {
		var openDropdown = dropdowns[i];
		var openDropdownBtn = dropdownBtn[i];

		if (openDropdown.classList.contains('show')) {
			openDropdown.classList.remove('show');
		}
		if (openDropdownBtn.classList.contains('open')) {
			openDropdownBtn.classList.remove('open');
		}

	}
}

function clickDropBtn(event) {
  if (!event.target.matches('.dropbtn')) {
	event.stopPropagation();
	removeOpen();
	}
}

// Close the dropdown if the user clicks outside of it
window.addEventListener("click", clickDropBtn)
</script>
